set MIXER deltaquad
set PWM_OUT 1234
set PWM_RATE 400

param set PWM_MAIN_DIS5 1500
param set PWM_MAIN_DIS6 1500
param set PWM_MAIN_DIS7 900
param set PWM_MAIN_DIS8 900

